.journalHead
  height 40px
  line-height 40px
  padding-left 40px
  >.title
    color #666
    font-size 22px
    font-weight 600
    font-style italic
    margin-right 10px
  >a
    display inline-block
    width 30px
    height 30px
    vertical-align middle
    background-image url("../../assets/svg/journal-back.svg")
    background-size cover
    transform rotate(90deg)
    transition 0.5s transform ease
    &:hover
      transform rotate(0deg)
.journalContent
  position relative

  